AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,KApFA;AAqFA,IAAA,UArFA,sBAqFA,CArFA,EAqFA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AADA;AAAA,uBAEA,MAAA,CAAA,KAAA,CACA,MADA,CACA,iCAAA,CAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,iBALA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,iBARA,CAFA;;AAAA;AAYA,gBAAA,MAAA,CAAA,QAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAlGA;AAmGA,IAAA,WAnGA,yBAmGA;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,SAAA,EAAA,SAAA,GAAA,EAAA;AACA;AArGA,GAzBA;AAgIA,EAAA,UAAA,EAAA;AAhIA,CAAA","sourcesContent":["<template>\n  <div class=\"main\">\n    <div class=\"tableName\">\n      <h3>&nbsp; List of Users</h3>\n      <button class=\"btn btn-primary btn-sm\" @click=\"addUser()\">Add</button>\n    </div>\n    <div class=\"container-fluid\">\n      <div class=\"row\" id=\"header\">\n        <div class=\"col-sm-1\"><h4>Id</h4></div>\n        <div class=\"col-sm-7\"><h4>Name</h4></div>\n        <div class=\"col-sm-4\"><h4>Actions</h4></div>\n      </div>\n      <div class=\"row\" v-for=\"(i, index) in record\" :key=\"index\">\n        <div class=\"col-sm-1\">\n          {{ i.id }}\n        </div>\n        <div class=\"col-sm-7\">{{ i.fname }} {{ i.mname }} {{ i.lname }}</div>\n        <div class=\"col-sm-4\" style=\"align-content: center\">\n          <button class=\"btn btn-success btn-sm\" data-toggle=\"modal\" data-target=\".bd-example-modal-lg\">\n            Show\n          </button>\n          <button class=\"btn btn-primary btn-sm\" @click=\"editUser(i)\">\n            Edit\n          </button>\n          <button class=\"btn btn-danger btn-sm\" @click=\"deleteUser(i)\">\n            Delete\n          </button>\n        </div>\n      </div>\n      <!-- <div class=\"col-xs-1\">\n        <button class=\"btn btn-success btn-xs\" @click=\"getUser(i)\">Show</button>\n      </div>\n      <div class=\"col-xs-1\">\n        <button class=\"btn btn-primary btn-xs\" @click=\"editUser(i)\">Edit</button>\n      </div>\n      <div class=\"col-xs-1\">\n        <button class=\"btn btn-danger btn-xs\" @click=\"deleteUser(i)\">Delete</button>\n      </div> -->\n    </div>\n    <!-- <button\n      type=\"button\"\n      class=\"btn btn-primary\"\n      data-toggle=\"modal\"\n      data-target=\".bd-example-modal-lg\"\n    >\n      Large modal\n    </button> -->\n\n    <div\n      class=\"modal fade bd-example-modal-lg\"\n      tabindex=\"-1\"\n      role=\"dialog\"\n      aria-labelledby=\"myLargeModalLabel\"\n      aria-hidden=\"true\"\n    >\n      <div class=\"modal-dialog modal-lg\">\n        <div class=\"modal-content\"> ... </div>\n      </div>\n    </div>\n    <!-- <div id=\"address\" v-if=\"isBtnClicked\">\n      <div>\n        {{ address }}\n        <button id=\"close\" @click=\"closeAddDiv()\">Close</button>\n      </div>\n    </div> -->\n    <div v-if=\"isDelClicked\">{{ msg }}</div>\n    <form v-if=\"isEditClicked || isAddClicked\">\n      <div>\n        <label>First Name:</label>\n        <input v-model=\"fname\" id=\"fname\" name=\"F_name\" type=\"text\" />\n      </div>\n      <div>\n        <label>Last Name:</label>\n        <input v-model=\"lname\" id=\"lname\" name=\"L_name\" type=\"text\" />\n      </div>\n      <div>\n        <label>Middle Name:</label>\n        <input v-model=\"mname\" id=\"mname\" name=\"M_name\" type=\"text\" />\n      </div>\n      <div>\n        <button\n          v-if=\"isformbtn\"\n          type=\"submit\"\n          id=\"Post\"\n          @click=\"editdata($event)\"\n        >\n          Edit\n        </button>\n        <button\n          v-if=\"!isformbtn\"\n          type=\"submit\"\n          id=\"Post\"\n          @click=\"editdata($event)\"\n        >\n          Add\n        </button>\n      </div>\n    </form>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      record: \"\",\n      address: null,\n      isBtnClicked: false,\n      isEditClicked: false,\n      isAddClicked: false,\n      isDelClicked: false,\n      fname: \"\",\n      lname: \"\",\n      mname: \"\",\n      id: \"\",\n      isformbtn: false,\n      msg: \"\",\n      // \"user1\",\n      // \"user2\",\n      // \"u\n    };\n  },\n  mounted() {\n    this.getUsers();\n  },\n\n  methods: {\n    getUsers() {\n      this.axios\n        .get(\"http://localhost:8000/Users/\")\n        .then((res) => {\n          this.record = res.data.data;\n          console.log(res);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      console.log(this.fname);\n    },\n\n    getUser(i) {\n      this.axios\n        .get(\"http://localhost:8000/Users/\" + i.id)\n        .then((res) => {\n          var address = [res.data.Address];\n          console.log(address);\n          this.isBtnClicked = true;\n          this.address = address;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      this.isBtnClicked = false;\n    },\n    async editdata(event) {\n      event.preventDefault();\n      this.bodyFormData = new FormData();\n\n      this.bodyFormData.append(\"F_name\", this.fname);\n      this.bodyFormData.append(\"M_name\", this.mname);\n      this.bodyFormData.append(\"L_name\", this.lname);\n      if (this.isEditClicked) {\n        await this.axios({\n          method: \"put\",\n          url: \"http://localhost:8000/Users/\" + this.id.id,\n          data: this.bodyFormData,\n          headers: { \"Content-Type\": \"multipart/form-data\" },\n        })\n          .then(function (response) {\n            //handle success\n            console.log(response);\n          })\n          .catch(function (err) {\n            //handle error\n            console.log(err);\n          });\n        this.isEditClicked = false;\n      } else {\n        await this.axios({\n          method: \"post\",\n          url: \"http://localhost:8000/Users/\",\n          data: this.bodyFormData,\n          headers: { \"Content-Type\": \"multipart/form-data\" },\n        })\n          .then(function (response) {\n            //handle success\n            console.log(response);\n          })\n          .catch(function (err) {\n            console.log(err);\n          });\n        this.isAddClicked = false;\n      }\n      this.getUsers();\n    },\n    editUser(i) {\n      this.isEditClicked = true;\n      this.isformbtn = true;\n      this.id = i;\n      this.fname = i.fname;\n      this.lname = i.lname;\n      this.mname = i.mname;\n    },\n    addUser(i) {\n      this.isAddClicked = true;\n      this.isformbtn = false;\n      this.id = i;\n      this.fname = \"\";\n      this.lname = \"\";\n      this.mname = \"\";\n    },\n    async deleteUser(i) {\n      this.isDelClicked = true;\n      await this.axios\n        .delete(\"http://localhost:8000/Users/\" + i.id)\n        .then((res) => {\n          this.msg = res.data.message;\n          console.log(res);\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n\n      this.getUsers();\n    },\n    closeAddDiv() {\n      document.getElementById(\"address\").innerHTML = \"\";\n    },\n  },\n  components: {},\n};\n</script>\n\n<style>\n.row {\n  margin: 1%;\n  padding-top: 1%;\n  border-top: 0.5px solid #a2a3a3;\n}\n.btn {\n  margin-left: 1%;\n  margin-right: 1%;\n}\n.tableName {\n  margin: 1%;\n  display: flex;\n  gap: 2%;\n}\n.main {\n  width: 90%;\n  margin: auto;\n  margin-top: 2%;\n  background-color: rgb(250, 249, 248);\n  border: 2px solid #a2a3a3;\n  border-radius: 12px;\n}\n</style>\n"],"sourceRoot":"src"}]}